@{
    ViewData["Title"] = "Contact";
}

<!-- Page Header Start -->
<section class="page-header">
    <div class="page-header__bg" style="background-image: url(../assets/images/backgrounds/ContactUs.jpg);">
    </div>
    <div class="container">
        <div class="page-header__inner">
            <h2>Get In Touch</h2>
            <div class="thm-breadcrumb__inner">
                <ul class="thm-breadcrumb list-unstyled">
                    <li><a asp-action="Index" asp-controller="Home">Home</a></li>
                    <li><span>/</span></li>
                    <li>Contact</li>
                </ul>
            </div>
        </div>
    </div>
</section>
<!-- Page Header End -->
<!-- Contact Page Start -->
<section class="contact-page">
    <div class="container">
        <div class="row">
            <!-- Contact Info - Phone -->
            <div class="col-xl-4 col-lg-4">
                <div class="contact-page__single">
                    <h3 class="contact-page__title">
                        <a href="tel:+962779444488">Tel: +962779444488</a>
                    </h3>
                    <p class="contact-page__text">
                        To get in touch with us, please submit
                        <br> the form below
                    </p>
                    <div class="contact-page__icon">
                        <span class="icon-call"></span>
                    </div>
                </div>
            </div>

            <!-- Contact Info - Email -->
            <div class="col-xl-4 col-lg-4">
                <div class="contact-page__single contact-page__single-3">
                    <h3 class="contact-page__title">
                        <a href="mailto:ysnedawi@gmail.com">ysnedawi@gmail.com</a>
                    </h3>
                    <p class="contact-page__text">
                        To better connect with our team
                        <br> send your mail.
                    </p>
                    <div class="contact-page__icon">
                        <span class="icon-open"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Contact Page End -->
